{
  "name": "SaaSServerless-common-api",
  "version": "1.0.0",
  "description": "SaaSServerless serverless api / customAuthorizer functions",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "offline": "sls offline start --stage localdev --port 3100 --noTimeout",
    "localdb": "serverless dynamodb start --stage local --port 8000 --dbPath ./LocalDB",
    "installCommon": "npm install",
    "installServiceDsc": "cd ../serviceDiscovery && npm install",
    "install:AuthMgr": "cd ../authMgr && npm install",
    "install:OrderMgr": "cd ../orderMgr && npm install",
    "install:ProductMgr": "cd ../productMgr && npm install",
    "install:SysReg": "cd ../sysReg && npm install",
    "install:TenantMgr": "cd ../tenantMgr && npm install",
    "install:TenantReg": "cd ../tenantReg && npm install",
    "install:UserMgr": "cd ../userMgr && npm install",
    "installDone": "echo '********** ALL DONE ***************'",
    "installAll": "npm-run-all installCommon install:* installDone",
    "deployCommon": "sls deploy",
    "deployServiceDsc": "cd ../serviceDiscovery && sls deploy",
    "deploy:AuthMgr": "cd ../authMgr && sls deploy",
    "deploy:OrderMgr": "cd ../orderMgr && sls deploy",
    "deploy:ProductMgr": "cd ../productMgr && sls deploy",
    "deploy:SysReg": "cd ../sysReg && sls deploy",
    "deploy:TenantMgr": "cd ../tenantMgr && sls deploy",
    "deploy:TenantReg": "cd ../tenantReg && sls deploy",
    "deploy:UserMgr": "cd ../userMgr && sls deploy",
    "deployDone": "echo '********** ALL DONE ***************'",
    "deployAll": "npm-run-all deployCommon deploy:* deployDone",
    "removeCommon": "sls remove",
    "removeServiceDsc": "cd ../serviceDsc && sls remove",
    "remove:AuthMgr": "cd ../authMgr && sls remove",
    "remove:OrderMgr": "cd ../orderMgr && sls remove",
    "remove:ProductMgr": "cd ../productMgr && sls remove",
    "remove:SysReg": "cd ../sysReg && sls remove",
    "remove:TenantMgr": "cd ../tenantMgr && sls remove",
    "remove:TenantReg": "cd ../tenantReg && sls remove",
    "remove:UserMgr": "cd ../userMgr && sls remove",
    "removeDone": "echo '********** ALL DONE ***************'",
    "removeAll": "npm-run-all  remove:* removeCommon deployDone"
  },
  "author": "Matt Ortiz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "serverless-offline": "^3.18.0",
    "serverless-webpack": "^5.1.0",
    "webpack": "^4.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "jsonwebtoken": "^7.3.0",
    "jwk-to-pem": "^1.2.6",
    "jwt-decode": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.81.0"
  }
}
